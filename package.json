{
	"name": "gifsicle-wrapper",
	"version": "4.1.2",
	"description": "Wrapper for Gifsicle",
	"license": "MIT",
	"repository": "lesjoursfr/gifsicle-wrapper",
	"homepage": "https://github.com/lesjoursfr/gifsicle-wrapper#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/gifsicle-wrapper/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "18.x || 20.x"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"install/**/*",
		"vendor/v1.94/source/gifsicle.tar.gz",
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"postinstall": "node install/install.js",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check \"**/*.{ts,js,cjs,mjs,json,yml,md}\"",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write \"**/*.{ts,js,cjs,mjs,json,yml,md}\"",
		"build-postinstall": "tsc --project tsconfig.install.json",
		"build": "tsc",
		"test": "NODE_OPTIONS='--loader=ts-node/esm' mocha"
	},
	"keywords": [
		"gif",
		"gifsicle",
		"image",
		"img",
		"compress",
		"resize",
		"minify",
		"optimize"
	],
	"type": "module",
	"dependencies": {
		"@lesjoursfr/bin-wrapper": "^12.1.2",
		"execa": "^8.0.1",
		"image-size": "^1.1.0",
		"tar": "^6.2.0",
		"tempy": "^3.1.0"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.2",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.10.6",
		"@types/tar": "^6.1.10",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"mocha": "^10.2.0",
		"prettier": "^3.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"packageManager": "yarn@4.0.2"
}
