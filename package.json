{
	"name": "gifsicle-wrapper",
	"version": "2.0.2",
	"description": "Wrapper for Gifsicle",
	"license": "MIT",
	"repository": "lesjoursfr/gifsicle-wrapper",
	"homepage": "https://github.com/lesjoursfr/gifsicle-wrapper#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/gifsicle-wrapper/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.15.0"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"build": "npx tsc",
		"lint": "npx eslint --ext .ts --fix src/",
		"test": "ava"
	},
	"keywords": [
		"gif",
		"gifsicle",
		"image",
		"img",
		"compress",
		"resize",
		"minify",
		"optimize"
	],
	"type": "module",
	"dependencies": {
		"execa": "^6.1.0",
		"gifsicle": "^6.1.0",
		"image-size": "^1.0.1"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/gifsicle": "^5.2.0",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"ava": "^4.2.0",
		"eslint": "^8.13.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"packageManager": "yarn@3.2.0"
}
