{
	"name": "@lesjoursfr/gifsicle-wrapper",
	"version": "4.1.6",
	"description": "Wrapper for Gifsicle",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lesjoursfr/gifsicle-wrapper.git"
	},
	"homepage": "https://github.com/lesjoursfr/gifsicle-wrapper#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/gifsicle-wrapper/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "20.x || 22.x"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"install/**/*",
		"vendor/v1.94/source/gifsicle.tar.gz",
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"postinstall": "node install/install.js",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build-postinstall": "tsc --project tsconfig.install.json",
		"build": "tsc",
		"test": "NODE_OPTIONS='--loader=ts-node/esm' mocha"
	},
	"keywords": [
		"gif",
		"gifsicle",
		"image",
		"img",
		"compress",
		"resize",
		"minify",
		"optimize"
	],
	"type": "module",
	"dependencies": {
		"@lesjoursfr/bin-wrapper": "^12.1.6",
		"execa": "^9.5.1",
		"image-size": "^1.1.1",
		"tar": "^7.4.3",
		"tempy": "^3.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.15.0",
		"@tsconfig/node20": "^20.1.4",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.10.1",
		"@types/tar": "^6.1.13",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.12.0",
		"mocha": "^10.8.2",
		"prettier": "^3.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"packageManager": "yarn@4.5.3"
}
